{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/aria": [
      "default",
      {
        "aria-input-field-name": "off",
        "aria-required-children": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "button-type": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "disown-opener": "off",
    "no-inline-styles": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "compat-api/css": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 113",
    "not chrome <= 113",
    "not edge <= 113",
    "not opera <= 98",
    "not samsung <= 20"
  ]
}